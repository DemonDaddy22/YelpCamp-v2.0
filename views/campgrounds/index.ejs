<% layout('layouts/boilerplate.ejs') %>
<% block('head').append(`<title>Campground</title>`) %>
    <h1>All Campgrounds</h1>
    <% for (let campground of campgrounds) { %>
        <li>
            <a href="/campgrounds/<%= campground?.id %>">
                <%= campground?.title %>
            </a>
        </li>
        <% } %>